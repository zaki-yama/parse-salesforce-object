{
  "name": "parse-salesforce-object",
  "version": "0.0.6",
  "description": "Parse Salesforce Custom Object's metadata and convert to Markdown/CSV/SOQL format",
  "author": "Shingo Yamazaki",
  "bin": {
    "parse-salesforce-object": "./bin/run"
  },
  "bugs": "https://github.com/zaki-yama/parse-salesforce-object/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "json2csv": "^5.0.5",
    "markdown-table": "^2.0.0",
    "string-width": "^4.2.0",
    "tslib": "^1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/json2csv": "^5.0.1",
    "@types/markdown-table": "^2.0.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/string-width": "^4.0.1",
    "@types/xml2js": "^0.4.7",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "npm-run-all": "^4.1.5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/zaki-yama/parse-salesforce-object",
  "keywords": [
    "salesforce",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "parse-salesforce-object"
  },
  "repository": "zaki-yama/parse-salesforce-object",
  "scripts": {
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint . --ext .ts --config .eslintrc",
    "lint:ts": "tsc --noEmit",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
