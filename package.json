{
  "name": "parse-salesforce-object",
  "description": "Parse Salesforce Custom Object's metadata and convert to Markdown/CSV/SOQL format",
  "version": "0.0.6",
  "author": "Shingo Yamazaki",
  "bin": {
    "parse-salesforce-object": "./bin/run"
  },
  "bugs": "https://github.com/zaki-yama/parse-salesforce-object/issues",
  "dependencies": {
    "@oclif/command": "^1.4.6",
    "@oclif/config": "^1.3.62",
    "@oclif/plugin-help": "^1.2.2",
    "json2csv": "^4.0.2",
    "markdown-table": "^1.1.1",
    "string-width": "^2.1.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/test": "^1.0.1",
    "@oclif/tslint": "^1.0.2",
    "@types/chai": "^4.1.2",
    "@types/json2csv": "^4.0.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/xml2js": "^0.4.2",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "release-it": "^7.3.0",
    "ts-node": "5",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/zaki-yama/parse-salesforce-object",
  "keywords": [
    "salesforce",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "parse-salesforce-object"
  },
  "preferGlobal": true,
  "repository": "zaki-yama/parse-salesforce-object",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepublishOnly": "yarn run build",
    "release": "release-it",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
